    <None Include="wwwroot\lib\Choices.js\.github\PULL_REQUEST_TEMPLATE.md" />
    <None Include="wwwroot\lib\Choices.js\.github\release-drafter.yml" />
    <None Include="wwwroot\lib\Choices.js\.github\workflows\browsers.yml" />
    <None Include="wwwroot\lib\Choices.js\.github\workflows\bundlesize.yml" />
    <None Include="wwwroot\lib\Choices.js\.github\workflows\deploy-pages.yml" />
    <None Include="wwwroot\lib\Choices.js\.github\workflows\deployment.yml" />
    <None Include="wwwroot\lib\Choices.js\.github\workflows\lint.yml" />
