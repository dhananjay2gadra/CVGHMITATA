    <None Include="wwwroot\lib\Choices.js\.github\workflows\release-drafter.yml" />
    <None Include="wwwroot\lib\Choices.js\.github\workflows\unit-tests.yml" />
    <None Include="wwwroot\videos\star.mp4" />
